<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/resultado"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <Button
        android:id="@+id/buttonCompilar2"
        android:layout_width="318dp"
        android:layout_height="56dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="132dp"
        android:onClick="verReporteGraficas"
        android:text="Ver Reporte de Graficas"
        android:textColor="@color/white"
        app:backgroundTint="@color/teal_200"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.493"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Texto"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="UsingOnClickInXml"/>

    <Button
        android:id="@+id/buttonCompilar3"
        android:layout_width="318dp"
        android:layout_height="56dp"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="60dp"
        android:onClick="verReporteOperadores"
        android:text="Ver Reporte de Operadores"
        android:textColor="@color/white"
        app:backgroundTint="@color/teal_700"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.494"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/Texto"
        app:layout_constraintVertical_bias="0.0"
        tools:ignore="UnknownIdInLayout,UsingOnClickInXml"/>

</androidx.constraintlayout.widget.ConstraintLayout>